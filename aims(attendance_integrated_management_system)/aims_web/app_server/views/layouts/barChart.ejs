<div class="panel-body">
    <div class="row">
        <div class="col-lg-12">
            <div class="table-responsive">
                <table class="table table-bordered table-hover table-striped">
                    <thead>
                        <tr>
                            <th>출석자</th>
                            <th>Date</th>
                            <th>Time</th>
                            <th>비고</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(var i = 0; i < HistoryViewCount; i++){ %>
                            <% if(attendanceHistory[i].check_schema[attendanceHistory[i].check_schema.length - 1].time == "00:00"){ %>
                                <% continue; %>
                            <% } %>
                        <tr>
                            <td><%= attendanceHistory[i].name %></td>
                            <td>
                            <%= attendanceHistory[i].check_schema[attendanceHistory[i].check_schema.length - 1].date %>
                            </td>
                            <td>
                            <%= attendanceHistory[i].check_schema[attendanceHistory[i].check_schema.length - 1].time %> AM
                            </td>
                            <td></td>
                        </tr>
                        <%}%>
                    </tbody>
                </table>
            </div>
            <!-- /.table-responsive -->
        </div>
        <!-- /.col-lg-4 (nested) -->
    
        <!-- /.col-lg-8 (nested) -->
